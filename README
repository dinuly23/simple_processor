файл:    alu.v		реализация АЛУ
файл:    control.v	управляющий автомат (УА)
файл:    count.v	увеличивает на 1
файл:    dm.v		реализация DM
файл:    ext0.v		без знаковое расширение
файл:    ext01.v	знаковое расширение по сигналу
файл:    fsm.v		автомат, реагирующий на step и change
файл:    im.v		реализация IM
файл:    main.v		операционный автомат (ОА)
файл:    main_mips.v	блок, соединяющий fsm и mips; основной блок
файл:    mips.v		блок, соединяющий OA и УА
файл:    mux.v		мультиплексор
файл:    pc.v		реализация PC
файл:    rf.v		реализация RF
файл:    sum.v		сумматор
файл:    test.v		тестовый блок

запуск: $ iverilog test.v
	$ ./a.out
	$ gtkwave dump
